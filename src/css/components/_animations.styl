@keyframes rotate {
    0% {
        transform rotate(0deg) translateZ(0)
    }
    100% {
        transform rotate(360deg) translateZ(0)
    }
}

@keyframes dash {
    0% {
        stroke-dasharray: 1, 200;
        stroke-dashoffset: 0
    }
    30% {
        stroke-dasharray: 89, 200;
        stroke-dashoffset: -35px
    }
    100% {
        stroke-dasharray: 89, 200;
        stroke-dashoffset: -124px
    }
}